'global':
  'editor':
    'fontSize': 13
    'fontFamily': 'monaco'
    'tabLength': 4
    'softWrap': true
    'lineHeight': 1.4
    'showIndentGuide': true
    'showInvisibles': true
    'invisibles':
      'tab': '>'
      'cr': ' '
      'eol': ' '
      'space': ' '
  'core':
    'disabledPackages': [
      'metrics'
      'exception-reporting'
    ]
  'welcome':
    'showOnStartup': false
  'atom-jshint':
    'hintOnModify': false
